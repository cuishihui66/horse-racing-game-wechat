// .env (example - create this file in your backend/ directory)
PORT=3000
JWT_SECRET=super_secret_key_for_jwt_prod_change_this_too
WECHAT_APP_ID=wxYOURAPPID
WECHAT_APP_SECRET=YOURWECHATAPPSECRET

# PostgreSQL running in Docker
DB_HOST=localhost
DB_PORT=5433 # Mapped from container's 5432
DB_USERNAME=postgres
DB_PASSWORD=mysecretpassword
DB_DATABASE=postgres # Default database in postgres:13

# Redis running in Docker
REDIS_HOST=localhost
REDIS_PORT=6379

MINIPROGRAM_BASE_URL=http://localhost:8080/path/to/mini-program/join?sessionId= # Replace with your actual miniprogram join URL